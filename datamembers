class A {
    int num = 10;
}
class B extends A {
    int num = 20;
}
class C extends B {
    int num = 30;
}
public class MainClass {
    public static void main(String[] args) {
        A ref; 
 ref = new B(); 
        System.out.println("Value of num: " + ref.num); 
 ref = new C(); 
        System.out.println("Value of num: " + ref.num); 
    }
}
